<div class="col s12">
  <div class="card">
    <div class="card-content flow-text">
      <div class="card-title">
        <h4><%=raw project.title %></h4>
      </div>
      <%=raw project.description %>
    </div>
    <div class="card-action">
      <%= link_to project.link_text, project.link %>
    </div>
  </div>
</div>
